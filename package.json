{
    "name": "cosmatic-app-oman",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3000",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
        "type-check": "tsc --noEmit",
        "test": "vitest",
        "test:unit": "vitest run",
        "test:unit:watch": "vitest watch",
        "test:unit:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:auth": "playwright test auth-persistence.spec.ts",
        "analyze": "node scripts/analyze-bundle.js analyze",
        "analyze:baseline": "npm run build && node scripts/analyze-bundle.js baseline",
        "analyze:compare": "npm run build && node scripts/analyze-bundle.js compare",
        "build:analyze": "npm run build && npm run analyze"
    },
    "dependencies": {
        "@directus/sdk": "^20.1.0",
        "@paypal/checkout-server-sdk": "^1.0.3",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.0.3",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.0.4",
        "@radix-ui/react-toast": "^1.2.15",
        "axios": "^1.6.5",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "date-fns": "^3.0.6",
        "framer-motion": "^11.0.3",
        "lucide-react": "^0.546.0",
        "next": "^16.0.1",
        "next-intl": "^4.4.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "swr": "^2.2.4",
        "tailwind-merge": "^2.2.0",
        "tailwindcss": "^3.4.1",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.3.3",
        "zustand": "^4.5.0"
    },
    "devDependencies": {
        "@babel/runtime": "^7.24.0",
        "@playwright/test": "^1.56.1",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/node": "20.19.23",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.1.0",
        "@vitest/ui": "^1.1.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^9.16.0",
        "eslint-config-next": "^16.0.1",
        "jsdom": "^23.0.1",
        "playwright": "^1.56.1",
        "postcss": "^8.4.33",
        "tailwindcss-rtl": "^0.9.0",
        "typescript": "^5.3.3",
        "vitest": "^1.1.0"
    }
}